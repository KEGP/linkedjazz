@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix ca: <http://localhost:8080/ontology/chord-annotations/> .
@prefix test: <.> .

test:cq_firstchord.ttl a owlunit:CompetencyQuestionVerification ;
	owlunit:hasCompetencyQuestion "Which is the first chord of the tune?" ;
	owlunit:hasSPARQLUnitTest "PREFIX owlunit: <https://w3id.org/OWLunit/ontology/> PREFIX ca: <http://localhost:8080/ontology/chord-annotations/>\nPREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\n\nSELECT ?chordType WHERE { \n  ?section rdf:type ca:Section ; \n           ca:isFirst true ; \n           ca:madeOfProgression ?progression . \n  ?progression ca:hasBar ?bar . \n  ?bar ca:isFirst true ; \n       ca:hasChord ?chord . \n  ?chord ca:isFirst true ; \n         rdf:type ?chordType .\n}" ;
	owlunit:hasInputData test:data.ttl ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
	owlunit:testsOntology ca: ;
	owlunit:hasExpectedResult "{\"head\":{\"vars\":[\"chordType\"]},\"results\":{\"bindings\":[{\"chordType\":{\"type\":\"uri\",\"value\":\"http://localhost:8080/ontology/chord-annotations/resource/Chord/F:(b3,5,b7,11)\"}}]}}" .