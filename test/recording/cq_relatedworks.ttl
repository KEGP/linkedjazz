@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix ex: <.> .
@prefix kejazz: <http://localhost:8080/ontology/kejazz/> .

ex:cq_relatedworks.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "What are the works that are concretized by a recording?" ;
 	owlunit:hasSPARQLUnitTest " PREFIX owlunit: <https://w3id.org/OWLunit/ontology/> PREFIX kejazz: <http://localhost:8080/ontology/kejazz/> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> SELECT ?title WHERE { BIND(<http://localhost:8080/resource/kejazz/Recording/09620053-4631-4c90-8050-326761f0168f> AS ?recording) . ?recording dul:realizes ?work . ?work kejazz:hasTitle ?title . } " ;
	owlunit:hasInputData <http://localhost:8080/sparql/db/query> ;
	owlunit:hasInputTestDataCategory owlunit:SPARQLendpoint ;
  owlunit:hasExpectedResult "{\"head\": {\"vars\": [\"title\"]}, \"results\": {\"bindings\": [{\"title\": {\"type\": \"literal\", \"value\": \"My Favorite Things\"}}]}}";
  owlunit:testsOntology kejazz: .
