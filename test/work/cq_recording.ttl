@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix ex: <.> .
@prefix kejazz: <http://localhost:8080/ontology/kejazz/> .

ex:cq_recording.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "Which are the associated recordings to this work? By whom? Recorded when?" ;
 	owlunit:hasSPARQLUnitTest " PREFIX owlunit: <https://w3id.org/OWLunit/ontology/> PREFIX kejazz: <http://localhost:8080/ontology/kejazz/> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> SELECT ?recordingTitle ?creditedArtistName ?start ?end WHERE { BIND (<http://localhost:8080/resource/kejazz/Work/028fce1d-d787-3501-a6ad-cc572786abe1> as ?work) . ?recording dul:realizes ?work ; kejazz:hasTitle ?recordingTitle ; kejazz:isCreditedTo ?creditedArtist ; dul:hasTimeInterval ?when . ?creditedArtist kejazz:hasName ?creditedArtistName . ?when kejazz:hasIntervalStartDate ?start ; kejazz:hasIntervalEndDate ?end . } " ;
	owlunit:hasInputData <http://localhost:8080/sparql/db/query> ;
	owlunit:hasInputTestDataCategory owlunit:SPARQLendpoint ;
  owlunit:hasExpectedResult "{\"head\": {\"vars\": [\"recordingTitle\", \"creditedArtistName\", \"start\", \"end\"]}, \"results\": {\"bindings\": [{\"recordingTitle\": {\"type\": \"literal\", \"value\": \"King Porter Stomp\"}, \"creditedArtistName\": {\"type\": \"literal\", \"value\": \"Jelly Roll Morton\"}, \"start\": {\"type\": \"literal\", \"value\": \"1923-07-17\"}, \"end\": {\"type\": \"literal\", \"value\": \"1923-07-17\"}}]}}";
  owlunit:testsOntology kejazz: .
